{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser":true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "comma-dangle":"off",
        "linebreak-style":"off",
        "object-curly-spacing":"off",
        "require-jsdoc":"off",
        "arrow-parens":"off",
        "operator-linebreak":"off"
    },
    "extends": ["eslint:recommended", "google"]
}
